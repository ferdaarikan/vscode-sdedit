{
    "name": "sdedit",
    "displayName": "sdedit",
    "description": "VSCode extension for Quick Sequence Diagram Editor",
    "version": "0.0.1",
    "publisher": "ferdaarikan",
    "engines": {
        "vscode": "^1.13.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello", "onCommand:extension.renderSequenceDiagram", "onCommand:extension.viewSequenceDiagram"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [{ "command": "extension.sayHello", "title": "Hello World"},
                     { "command": "extension.renderSequenceDiagram", "title": "Render sequence diagram"},        
                     { "command": "extension.viewSequenceDiagram", "title": "View sequence diagram"}  
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "eslint": "^3.6.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}